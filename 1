for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
if v.ClassName == "Part" then
local args = {
    [1] = "}11, { \n\n } ",
    [2] = "}, { ",
    [3] = {
        ["Position"] = v,
        ["SoundId"] = "rbxassetid://8355194421",
        ["Replicate"] = true,
        ["Volume"] = 9999999,
        ["Effects"] = true
    }
}

game:GetService("ReplicatedStorage").ChatSystemMakeMessage:FireServer(unpack(args))
end
end
